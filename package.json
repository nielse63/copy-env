{
  "name": "@nielse63/copy-env",
  "version": "1.0.2",
  "description": "Automate managing example .env files",
  "main": "dist/cjs/index.js",
  "types": "dist/cjs/index.d.js",
  "exports": {
    "import": "./dist/mjs/index.js",
    "require": "./dist/cjs/index.js"
  },
  "files": [
    "dist",
    "cli.js"
  ],
  "bin": "./cli.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "jest",
    "prepare": "husky install",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "lint": "prettier --write ./src && eslint --fix ./src",
    "prebuild": "rm -rf dist/",
    "build": "tsc -p config/tsconfig.cjs.json && tsc -p config/tsconfig.mjs.json",
    "prepack": "npm run build",
    "docs": "typedoc --options config/typedoc.json",
    "postdocs": ".bin/docs-cleanup",
    "release": "dotenv release-it"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nielse63/copy-env.git"
  },
  "keywords": [
    "env",
    "dotenv",
    "cli",
    ".env",
    "config",
    "settings",
    "utils"
  ],
  "author": "Erik Nielsen <erik@312development.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nielse63/copy-env/issues"
  },
  "homepage": "https://github.com/nielse63/copy-env#readme",
  "engines": {
    "node": "^16.19.1",
    "npm": "^v8.19.3"
  },
  "dependencies": {
    "commander": "^10.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-typescript": "^7.21.5",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.1.0",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "babel-jest": "^29.5.0",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.40.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^27.0.1",
    "eslint-plugin-jest-extended": "^2.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "jest": "^29.5.0",
    "jest-extended": "^3.2.4",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "release-it": "^15.10.3",
    "ts-node": "^10.9.1",
    "typedoc": "^0.24.7",
    "typescript": "^5.0.4"
  },
  "lint-staged": {
    "*.{ts,js,json,md,yml}": "prettier --write"
  }
}
